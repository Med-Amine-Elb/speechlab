# Environment Configuration Template
# Copy this file to env.properties and fill in your actual values
# DO NOT commit env.properties to git - it contains sensitive data

# JWT Secret (generate a strong secret for production)
jwt.secret=your-jwt-secret-here-32-chars-minimum

# MongoDB Atlas Connection
spring.data.mongodb.uri=mongodb+srv://username:password@cluster.mongodb.net/database?retryWrites=true&w=majority&appName=app-name

# Deepgram API Key
api.deepgram.apiKey=your-deepgram-api-key-here

# Gemini API Key
api.gemini.apiKey=your-gemini-api-key-here
